{
    "name": "ziffmedia/laravel-eloquent-imagery",
    "description": "Image handling per column/attribute for Laravel Eloquent",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph_schindler@ziffdavis.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "intervention/image": "^2.4",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "ZiffMedia\\Laravel\\EloquentImagery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZiffMedia\\Laravel\\EloquentImagery\\Test\\": "tests/"
        }
    },
    "suggest": {
        "intervention/image": "Intervention Image is necessary for serving images that need modification",
        "laravel/framework": "This package is intended to be used with laravel/framework version >= 5.7"
    },
    "conflict": {
        "laravel/framework": "<5.7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZiffMedia\\Laravel\\EloquentImagery\\EloquentImageryProvider"
            ]
        }
    }
}
